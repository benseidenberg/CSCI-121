public class DinnerParty extends Party {
    public int dinnerChoice;

    public void setDinnerChoice(int num){
        dinnerChoice = num;
    }
    public String getDinnerChoice(){
        if (dinnerChoice == 1) {
            return "BBQ Ribs";
        }
        else {
            return "Chicken Alfredo";
        }
    }
}
