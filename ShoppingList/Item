//TIP To <b>Run</b> code, press <shortcut actionId="Run"/> or
// click the <icon src="AllIcons.Actions.Execute"/> icon in the gutter.
public class Item {

    String name;
    Float price;
    public Item(String name, Float price){
        this.name = name;
        this.price = price;
    }
    public String getName() {
        return name;
    }
    public Float getPrice() {
        return price;
    }

}
